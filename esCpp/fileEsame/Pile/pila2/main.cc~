using namespace std;

void readFile(char inputFile[]);

int main(int argc, char *argv[]) {
  if(argc != 3) {
    cout << "Numero parametri sbagliato" << endl;
    exit(0);
  }

  char inputFile[100] = argv[1];
  return 0;
}
